    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.13",
    "@nestpress/next": "^1.2.3",
    "@reach/accordion": "^0.13.2",
    "@reach/alert-dialog": "^0.13.2",
    "@reach/combobox": "^0.13.2",
    "@reach/dialog": "^0.13.2",
    "@reach/listbox": "^0.13.2",
    "@reach/tooltip": "^0.13.2",
    "@timkendrick/monaco-editor": "0.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-fetch": "^2.5.8",
    "ace-builds": "^1.4.12",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.7.0",
    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "codemirror": "^5.59.4",
    "cookie": "^0.4.1",
    "deep-diff": "^1.0.2",
    "dotenv": "^8.2.0",
    "javascript-terminal": "^1.1.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.22.3",
    "mysql": "^2.18.1",
    "nestjs-redis": "^1.2.8",
    "next": "^10.0.7",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "random-username-generator": "^1.0.4",
    "react": "^17.0.1",
    "react-ace": "^9.3.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.1",
    "react-monaco-editor": "^0.42.0",
    "react-spring": "^8.0.27",
    "react-terminal-component": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sass": "^1.32.8",
    "socket.io-client-cookies-headers": "^0.4.2",
    "socketio-wildcard": "^2.0.0",
    "sqlite3": "^5.0.2",
    "styled-components": "^5.2.1",
    "ts-debounce": "^3.0.0",
    "typeorm": "^0.2.31",
    "use-resize-observer": "^7.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@convergencelabs/ace-collab-ext": "^0.5.0",
    "@convergencelabs/codemirror-collab-ext": "^0.1.2",
    "@convergencelabs/monaco-collab-ext": "^0.3.1",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.3",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "^4.17.11",
    "@types/jest": "26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.35",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "autoprefixer": "^10.2.4",
    "babel-jest": "^26.6.3",
    "classnames": "^2.2.6",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.1",
    "jest": "26.6.3",
    "lint-staged": "^10.5.4",
    "mobx": "^6.1.7",
    "mobx-react": "^7.1.0",
    "mobx-utils": "^6.0.3",
    "moment": "^2.29.1",
    "monaco-editor-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.3",
    "react-service-provider": "^1.2.1",
    "react-test-renderer": "^17.0.1",
    "semantic-release": "^17.3.9",
    "styled-jsx": "^3.4.4",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.0.3",
    "ts-jest": "26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "transform": {
      "\\.ts$": [
        "ts-jest"
      ],
      "\\.tsx$": [
        "babel-jest",
        {
          "presets": [
            "next/babel"
          ],
          "plugins": [
            [
              "@babel/plugin-proposal-decorators",
              {
                "legacy": true
              }
            ]
          ]
        }
      ]
    },
    "rootDir": "src",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@env/(.*)$": "<rootDir>/env/test/$1",
      "~/(.*)$": "<rootDir>/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --fix",
      "prettier --write",
      "jest --findRelatedTests"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      ".next/**/*.js"
    ],
    "assets": [
      "dist/**/*.js.map",
      "public/**/*",
      ".next/**/*"
    ]
  },
  "majestic": {
    "jestScriptPath": "./node_modules/.bin/jest",
    "env": {
      "ENV": "test"
    }
  }
}
